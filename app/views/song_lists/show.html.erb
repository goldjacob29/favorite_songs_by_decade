<div class="page-header">
  <h3>Song List #<%= @song_list.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Song</dt>
      <dd><%= @song_list.song_id %></dd>

      <dt>Playlist</dt>
      <dd><%= @song_list.playlist_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/song_lists" class="btn btn-primary">
        Back
      </a>
      <a href="/song_lists/<%= @song_list.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_song_list/<%= @song_list.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
